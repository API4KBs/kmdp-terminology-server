# baseURI: http://ontology.mayo.edu/ontologies/propositionalconcepts#
# imports: http://datashapes.org/dash
# imports: http://datashapes.org/graphql
# imports: http://www.w3.org/2004/02/skos/core
# prefix: PCVSchema

@prefix metadata: <http://topbraid.org/metadata#> .
@prefix olex:  <http://www.w3.org/ns/lemon/ontolex#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix PCVSchema: <http://ontology.mayo.edu/ontologies/propositionalconcepts#> .
PCVSchema:resolvable  a  owl:AnnotationProperty ;
        rdfs:domain  <http://www.w3.org/2004/02/skos/core#Concept> ;
        rdfs:range   xsd:boolean ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Concept that can be resolved in a specific context, applying a suitable (Operational) Definition"@en ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "resolvable"@en .

<http://purl.org/dc/elements/1.1/identifier>
        a            owl:AnnotationProperty ;
        rdfs:domain  rdfs:Resource .

olex:isConceptOf  a  owl:ObjectProperty ;
        rdfs:domain  <http://www.w3.org/2004/02/skos/core#Concept> .

rdfs:isDefinedBy  a  owl:AnnotationProperty ;
        rdfs:domain  rdfs:Resource .

PCVSchema:  a            owl:Ontology ;
        rdfs:label       "PCVSchema" ;
        metadata:status  metadata:UnderDevelopmentStatus ;
        <http://topbraid.org/swa#defaultNamespace>
                "http://ontology.mayo.edu/ontologies/propositionalconcepts#" ;
        owl:imports      <http://www.w3.org/2004/02/skos/core> , <http://datashapes.org/dash> , <http://datashapes.org/graphql> .